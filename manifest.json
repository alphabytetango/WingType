{
    "manifest_version": 2,
    "name": "Wingtype",
    "description": "A basic Hello World example extension!",
    "version": "1.0",
    "author": "sthomps@alphabytetango.com",
    "browser_specific_settings": {
        "gecko": {
            "id": "sthomps@alphabytetango.com",
            "strict_min_version": "78.0"
        }
    },
    "browser_action": {
        "default_popup": "mainPopup/popup.html",
        "default_title": "Wingtype",
        "default_icon": "images/icon-32px.png"
    },
    "icons": {
        "64": "images/icon.png",
        "32": "images/icon-32px.png",
        "16": "images/icon-16px.png"
    }
}